    <!-- http://identifiers.org/mamo/Mathematical_model -->

    <owl:Class rdf:about="&mamo;Mathematical_model">
        <rdfs:label>Mathematical_model</rdfs:label>
        <rdfs:subClassOf rdf:resource="&eko;KPICalculationComponent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eko;isMathematicalModelOf"/>
                <owl:onClass rdf:resource="&eko;KPI"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eko;containsAssumption"/>
                <owl:someValuesFrom rdf:resource="&eko;Assumption"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&eko;containsConstant"/>
                <owl:someValuesFrom rdf:resource="&eko;UniversalConstant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
