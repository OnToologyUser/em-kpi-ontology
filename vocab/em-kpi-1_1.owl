    <!-- http://energy.linkeddata.es/em-kpi/ontology#hasDeterminant -->

    <owl:ObjectProperty rdf:about="&eko;hasDeterminant">
        <rdfs:label xml:lang="en">hasDeterminant</rdfs:label>
        <rdfs:comment xml:lang="en">Relation linking Energy Cost to its determinants like Energy Supply and Energy Tariff</rdfs:comment>
        <owl:inverseOf rdf:resource="&eko;isDeterminantOf"/>
        <rdfs:domain rdf:resource="&ero;EnergyCost"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
