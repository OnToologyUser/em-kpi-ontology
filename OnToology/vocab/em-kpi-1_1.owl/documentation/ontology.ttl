              [ a       owl:Restriction ;
                owl:onProperty eko:isAssociatedObjectOf ;
                owl:someValuesFrom eko:KPICalculation
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (dul:PhysicalObject [ a       owl:Class ;
