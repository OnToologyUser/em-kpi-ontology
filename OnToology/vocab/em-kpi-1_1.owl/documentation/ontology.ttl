geo:Point
      a       owl:Class ;
      rdfs:comment """Uniquely identified by lat/long/alt. i.e.

spaciallyIntersects(P1, P2) :- lat(P1, LAT), long(P1, LONG), alt(P1, ALT),
  lat(P2, LAT), long(P2, LONG), alt(P2, ALT).

sameThing(P1, P2) :- type(P1, Point), type(P2, Point), spaciallyIntersects(P1, P2)."""@en , "A point, typically described using a coordinate system relative to Earth, such as WGS84, includes the latitude, altitude and longitude of a place."@en ;
      rdfs:label "GeographicalCoordinatesPoint" ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onDataRange xsd:string ;
                            owl:onProperty geo:alt ;
                            owl:qualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger
                          ] [ a       owl:Restriction ;
                            owl:onDataRange xsd:string ;
                            owl:onProperty geo:lat ;
                            owl:qualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger
                          ] [ a       owl:Restriction ;
                            owl:onDataRange xsd:string ;
                            owl:onProperty geo:long ;
                            owl:qualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger
                          ])
              ] .

