      rdfs:comment "To describe the unit of observation value and KPI value"@en ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (ssn:ObservationValue eko:KPIValue)
              ] ;
      rdfs:label "hasUnit"@en ;
      rdfs:range om:Unit_of_measure ;
      owl:inverseOf eko:isUnitOf .

ero:producesEnergyType
      a       owl:ObjectProperty ;
      rdfs:comment "Relation linking the energy generation system or equipment to the Energy Type ir produces"@en ;
      rdfs:domain eko:PowerSystemResource ;
      rdfs:label "producesEnergyType"^^xsd:string ;
      rdfs:range ero:EnergyType .

eko:isAssociatedObjectOf
      a       owl:InverseFunctionalProperty , owl:ObjectProperty ;
      rdfs:domain eko:KPIEvaluatedObject ;
      rdfs:label "isAssociatedObjectOf"@en ;
      rdfs:range eko:KPICalculation .

ero:BuildingThing
      a       owl:Class ;
      rdfs:comment "All things (here physical objects) that creates a building, it is intended as root of all building components."@en ;
      rdfs:label "BuildingThing"@en ;
      rdfs:subClassOf dul:PhysicalObject ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass bio:Building ;
                owl:onProperty ero:isIn ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

ero:EnergyFacility
      a       owl:Class ;
      rdfs:comment "The energy facility in building that consumes or produces energy"@en ;
      rdfs:label "EnergyFacility"@en ;
      rdfs:subClassOf ero:BuildingThing ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty ero:contains ;
                owl:someValuesFrom eko:BuildingEquipment
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty eko:isAssociatedObjectOf ;
                owl:someValuesFrom eko:KPICalculation
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (ero:BuildingThing [ a       owl:Class ;
                            owl:unionOf ([ a       owl:Restriction ;
                                        owl:onProperty ero:consumesEnergy ;
                                        owl:someValuesFrom ero:EnergyDemand
                                      ] [ a       owl:Restriction ;
                                        owl:onProperty ero:producesEnergy ;
                                        owl:someValuesFrom eko:EnergyProduction
                                      ])
                          ] [ a       owl:Class ;
                            owl:complementOf ero:Equipment
                          ])
              ] .

mamo:Mathematical_model
      a       owl:Class ;
      rdfs:comment "The mathematical model for KPI calculation. The model is composed of a set of variables and a set of equations that establish relationships between the variables. "@en ;
      rdfs:label "Mathematical_model" ;
      rdfs:subClassOf eko:KPICalculationComponent ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass eko:Equation ;
                owl:onProperty bio:containsEquation ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass eko:KPI ;
                owl:onProperty eko:isMathematicalModelOf ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty eko:containsConstant ;
                owl:someValuesFrom eko:UniversalConstant
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty eko:containsVariable ;
                owl:someValuesFrom mamo:Variable
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty eko:containsAssumption ;
                owl:someValuesFrom eko:Assumption
              ] .

eko:Battery
      a       owl:Class ;
      rdfs:comment "The set of batteries that stores energy."@en ;
      rdfs:label "Battery"@en ;
      rdfs:subClassOf eko:StoringUnit .

eko:containsAssumption
      a       owl:ObjectProperty ;
      rdfs:comment "The assumption of the mathematical model for KPI calculation"@en ;
      rdfs:domain mamo:Mathematical_model ;
      rdfs:label "containsAssumption"@en ;
      rdfs:range eko:Assumption ;
      owl:inverseOf eko:isAssumptionOf .

dc:identifier
